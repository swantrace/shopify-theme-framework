{
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es6": true,
    "jasmine": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "no-param-reassign": [
      "error",
      { "props": true, "ignorePropertyModificationsFor": ["state"] }
    ]
  }
}
